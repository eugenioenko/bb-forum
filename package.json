{
  "name": "bb-client-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=4200",
    "build": "next build",
    "start": "next start",
    "db": "docker-compose up -d",
    "lint": "next lint",
    "prisma:init": "prisma init",
    "prisma:migrate": "export DATABASE_URL=mysql://root:password@localhost:3306/bb && prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "test": "npx vitest",
    "test:headless": "npx vitest --run --browser.headless"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  },
  "dependencies": {
    "next": "14.2.9",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@floating-ui/react": "^0.26.25",
    "@hookform/resolvers": "^3.9.0",
    "@ngneat/falso": "^7.2.0",
    "@prisma/client": "^5.19.1",
    "@tabler/icons-react": "^3.16.0",
    "@tanstack/react-query": "^5.55.4",
    "@tanstack/react-query-devtools": "^5.55.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.0.5",
    "axios": "^1.7.7",
    "axios-cache-interceptor": "^1.6.0",
    "bcrypt": "^5.1.1",
    "dataloader": "^2.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.9",
    "jsdom": "^25.0.0",
    "jsonwebtoken": "^9.0.2",
    "msw": "^2.4.5",
    "playwright": "^1.47.0",
    "postcss": "^8.4.45",
    "prisma": "^5.19.1",
    "prisma-erd-generator": "^1.11.2",
    "react-hook-form": "^7.53.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
